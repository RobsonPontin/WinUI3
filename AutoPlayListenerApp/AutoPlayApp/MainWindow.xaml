<?xml version="1.0" encoding="utf-8"?>
<Window
    x:Class="AutoPlayApp.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:AutoPlayApp"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button x:Name="myButton" Margin="0 5 0 0" Click="myButton_Click">Get WPD Known devices</Button>
        <Button x:Name="btnTestComServer" Margin="0 5 0 0" Click="btnTestComServer_Click" >Test COM Server</Button>
        <Button x:Name="btnTestCmdLine" Margin="0 5 0 0" Click="btnTestCmdLine_Click">Test Command Line</Button>
    </StackPanel>
</Window>

    <!--Arch-specific implementation DLLs should be copied into RID-relative folders-->
    <file src="..\..\x64\Release\WinRt_cppcx\WinRt_cppcx.dll" target="runtimes\win10-x64\native\WinRt_cppcx.dll" />
  </files>
</package>